{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "cy:open": "cypress open",
    "debug": "node --nolazy --inspect-brk node_modules/.bin/gatsby develop",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "./node_modules/.bin/eslint *.js src/**/*.js",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open"
  },
  "dependencies": {
    "gatsby": "^2.15.26",
    "gatsby-image": "^2.2.24",
    "gatsby-plugin-sharp": "^2.2.28",
    "gatsby-source-filesystem": "^2.1.28",
    "gatsby-transformer-sharp": "^2.2.20",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/react": "^9.2.0",
    "axe-core": "^3.3.2",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.17",
    "cypress": "^3.4.1",
    "cypress-axe": "^0.5.1",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.0",
    "start-server-and-test": "^1.10.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
