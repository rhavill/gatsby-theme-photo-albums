{
  "name": "gatsby-theme-photo-albums",
  "private": false,
  "description": "A photo albums theme for Gatsby",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "cy:open": "cypress open",
    "debug": "node --nolazy --inspect-brk node_modules/.bin/gatsby develop",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "./node_modules/.bin/eslint *.js src/*.js src/**/*.js",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@mdx-js/react": "^1.5.1",
    "gatsby-image": "^2.2.29",
    "gatsby-plugin-sharp": "^2.2.32",
    "gatsby-plugin-theme-ui": "^0.2.43",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-transformer-sharp": "^2.3.0",
    "ramda": "^0.26.1",
    "theme-ui": "^0.2.49"
  },
  "peerDependencies": {
    "gatsby": "^2.17.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/react": "^9.3.0",
    "axe-core": "^3.4.0",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.20",
    "cypress": "^3.5.0",
    "cypress-axe": "^0.5.1",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "gatsby": "^2.17.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "npm-check": "^5.9.0",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-test-renderer": "^16.11.0",
    "start-server-and-test": "^1.10.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhavill/gatsby-theme-photo-albums"
  },
  "bugs": {
    "url": "https://github.com/rhavill/gatsby-theme-photo-albums/issues"
  }
